# -----------------------------------------------------
# AUTOSTART
# -----------------------------------------------------

# -----------------------------------------------------
# Fastfetch
# -----------------------------------------------------
if [[ $(tty) == *"pts"* ]]; then
    fastfetch --config examples/13
else
    echo
    if [ -f /bin/hyprctl ]; then
        echo "Запуск Hyprland с помощью команды Hyprland"
    fi
fi

# -----------------------------------------------------
# Starship
# -----------------------------------------------------
eval "$(starship init zsh)"

# -----------------------------------------------------
# Zoxide
# -----------------------------------------------------
eval "$(zoxide init --cmd cd zsh)"
